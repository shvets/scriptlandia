<?xml version="1.0"?>

<project>
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.sf.scriptlandia</groupId>
  <artifactId>fx-starter</artifactId>
  <version>2.2.2</version>
  <packaging>jar</packaging>

  <name>Scriptlandia JavaFX Starter Module</name>
  <description>Scriptlandia JavaFX Starter Module</description>

  <dependencies>
    <dependency>
      <groupId>org.sf.scriptlandia</groupId>
      <artifactId>scriptlandia-helper</artifactId>
      <version>2.2.2</version>
    </dependency>

    <dependency>
      <groupId>org.sf.scriptlandia</groupId>
      <artifactId>fx</artifactId>
      <version>2.2.2</version>
      <type>pom</type>
    </dependency>

    <dependency>
      <groupId>net.java.dev.swing-layout</groupId>
      <artifactId>swing-layout</artifactId>
      <version>1.0.1</version>
    </dependency>

    <!--dependency>
      <groupId>com.jhlabs</groupId>
      <artifactId>filters</artifactId>
      <version>2.0.235</version>
    </dependency-->
  </dependencies>

  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>${java.specification.version}</source>
          <target>${java.specification.version}</target>
        </configuration>
      </plugin>
    </plugins>
  </build>

</project>
