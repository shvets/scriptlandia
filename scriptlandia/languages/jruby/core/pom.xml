<?xml version="1.0"?>

<project>
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.sf.scriptlandia</groupId>
  <artifactId>jruby</artifactId>
  <version>2.2.5</version>
  <packaging>pom</packaging>

  <name>Scriptlandia JRuby Core Module</name>
  <description>Scriptlandia JRuby Core Module</description>

  <properties>
    <jruby.version>1.1.4</jruby.version>
  </properties>

  <dependencies>
    <dependency>
      <groupId>org.jruby</groupId>
      <artifactId>jruby</artifactId>
      <version>${jruby.version}</version>

      <exclusions>
        <exclusion>
          <groupId>org.jruby.extras</groupId>
          <artifactId>jna</artifactId>
        </exclusion>
      </exclusions>
    </dependency>

    <dependency>
      <groupId>org.jruby</groupId>
      <artifactId>jruby-lib</artifactId>
      <version>${jruby.version}</version>
    </dependency>
  </dependencies>

</project>
