<?xml version="1.0"?>

<language name="scriptlandia" groupId="org.sf.scriptlandia" artifactId="scriptlandia" version="2.2.6">
  <registration mimeType="application/scriptlandia" icon="scriptlandia.ico">
    <extensions>
      <extension>scriptlandia</extension>
      <extension>sland</extension>
    </extensions>

    <starter>
      <mainClass>org.sf.scriptlandia.ScriptlandiaStarter</mainClass>
      <commandLine>-script.name</commandLine>
    </starter>

    <script name="sland-run"/>
  </registration>
</language>
