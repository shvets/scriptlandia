# List of Deploy related C++ source files
DEPLOY_FILES_cpp =         \
        BasicPerfStore.cpp \
        DeployPerf.cpp     \
        PerfHelper.cpp     \
        PerfLabel.cpp

ifeq ($(PLATFORM),windows)
    # List of Windows specific DeployPerf related C++ source files
    DEPLOY_FILES_cpp +=        \
        auth.cpp               \
	common.cpp             \
        config.cpp             \
        console.cpp            \
        cookie.cpp             \
        dllmain.cpp            \
        jscriptengine.cpp      \
        offline.cpp            \
        proxy.cpp              \
        registry.cpp           \
        security.cpp           \
        stdafx.cpp             \
        WinRegistryWrapper.cpp \
        WinTime.cpp
else
    # List of Unix specific Deploy related C++ source files
    DEPLOY_FILES_cpp += \
        UnixTime.cpp

    # List of Unix specific Deploy related C source files
    DEPLOY_FILES_c +=         \
        config.c              \
        console.c             \
        GnomeVfsWrapper.c     \
        MSystemProxyHandler.c \
        NetworkUtil.c
endif

DEPLOY_FILES_rc = version.rc
