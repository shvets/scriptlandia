<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="User" table="User">
    <id name="id" column="Id">
      <generator class="assigned"/>
    </id>

    <property name="name">
      <column name="Name"/>
    </property>

    <property name="age">
      <column name="Age"/>
    </property>
  </class>
</hibernate-mapping>
