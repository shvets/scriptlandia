import f3.ui.*;

class HelloWorldModel {
    attribute saying: String;
}

var model = HelloWorldModel {
    saying: "Hello World"
};

var win = Frame {

    title: "Hello World F3"

    width: 200
    height: 100

    content: Label {

        text: bind model.saying

    }

    visible: true
};

model.saying = "Goodbye Cruel World!";